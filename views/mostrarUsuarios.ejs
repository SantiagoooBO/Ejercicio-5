<%- include("templates/encabezado") %>
    <br>
    <h1 style="text-align: center;">Usuarios</h1>
    <br>
    <div class="contenedor">
        <table>
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Nombre</th>
                    <th>Celular</th>
                    <th>Correo</th>
                    <th>Editar / Borrar</th>
                </tr>
            </thead>
            <tbody>
                <%
                usuariosCorrectos.forEach((usuario)=>{
                %>
                <tr>
                    <td><%= usuario.idusuario %></td>
                    <td><%= usuario.nombre %></td>
                    <td><%= usuario.celular %></td>
                    <td><%= usuario.correo %></td>
                    <td>
                        <a href="/editarUsuario/<%= usuario.idusuario %>">Editar </a>
                        <a href="/borrarUsuario/<%= usuario.idusuario %>">Borrar</a>
                    </td>
                </tr>
                <%
                });
                %>
            </tbody>
        </table>
        
    </div>
    <br>
        <a href="/agregarusuario">Agregar Usuario</a>
        <a href="/Productos">Ver Productos</a>
        <%- include("templates/pie") %>